{
  "name": "myapp",
  "version": "0.6.0",
  "private": true,
  "description": "Ecommerce template",
  "author": "Sugam Karki",
  "scripts": {
    "build:client": "ng build",
    "build:server": "NODE_ENV=production nest build --config ./server/nest-cli.json",
    "prebuild:ssr": "ngcc",
    "build:ssr": "NODE_ENV=production ng build --configuration production && ng run myapp:server:production",
    "dev:backend": "ng run myapp:serve-ssr",
    "dev:frontend": "ng serve",
    "format": "prettier --write \"server/**/*.ts\"",
    "heroku-postbuild": "npm run build:ssr",
    "lint": "eslint \"{server,apps}/**/*.ts\" --fix",
    "lint:client": "ng lint",
    "migrate:dev": "prisma migrate dev",
    "prebuild": "rimraf dist",
    "prerender": "ng run myapp:prerender",
    "release": "standard-version",
    "serve:client:prod": "ng serve --configuration production",
    "serve:ssr": "node dist/myapp/server/main.js",
    "start": "npm run serve:ssr",
    "start:client": "ng serve",
    "start:debug": "nest start --debug --watch",
    "start:dev": "NODE_ENV=development nest start --watch config ./server/nest-cli.json --path ./server/tsconfig.json",
    "start:prod": "node server/dist/main",
    "start:server": "NODE_ENV=production nest start config ./server/nest-cli.json --path ./server/tsconfig.json"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ],
  "dependencies": {
    "@angular-builders/custom-webpack": "^13.1.0",
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "13.3.7",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/material": "13.3.7",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/platform-server": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@angular/service-worker": "^13.3.8",
    "@nestjs/common": "^8.4.5",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.5",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/ng-universal": "6.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.5",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/testing": "^8.4.5",
    "@nguniversal/express-engine": "^13.1.1",
    "@prisma/client": "^3.14.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.2",
    "express": "^4.15.2",
    "fastify-swagger": "^5.2.0",
    "passport": "^0.5.3",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.5.0",
    "sass": "^1.51.0",
    "swagger-ui-express": "^4.4.0",
    "tslib": "^2.3.0",
    "winston": "3.7.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular/cli": "~13.3.5",
    "@angular/compiler-cli": "~13.3.0",
    "@ngtools/webpack": "^13.3.5",
    "@nguniversal/builders": "^13.1.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.0.3",
    "@types/node": "^17.0.35",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jasmine-core": "~4.1.1",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^2.6.2",
    "prisma": "^3.14.0",
    "typescript": "~4.6.2",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  }
}
